{
  "name": "@coreui/vue",
  "description": "CoreUI Vue Bootstrap 4 layout components",
  "version": "3.0.0-beta.8",
  "license": "MIT",
  "main": "dist/coreui-vue.common.js",
  "types": "src/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "sideEffects": [
    "./src/components/toast/CToast.vue",
    "./src/components/scrollbar/CScrollbar.vue",
    "./src/components/alert/CAlert.vue",
    "./src/components/table/CDataTable.vue",
    "./src/components/tabs/CTabs.vue",
    "./src/components/toast/CCarousel.vue"
  ],
  "homepage": "https://coreui.io",
  "author": {
    "name": "CoreUI",
    "url": "https://coreui.io",
    "github": "https://github.com/coreui",
    "twitter": "https://twitter.com/core_ui"
  },
  "contributors": [
    {
      "name": "CoreUI Team",
      "url": "https://github.com/orgs/coreui/people"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/coreui/coreui-vue.git"
  },
  "bugs": {
    "url": "https://github.com/coreui/coreui-vue/issues"
  },
  "keywords": [
    "coreui",
    "vue",
    "vue-component",
    "vue-library",
    "bootstrap",
    "framework",
    "responsive",
    "layout",
    "component",
    "components"
  ],
  "engines": {
    "node": ">=8.9.0",
    "npm": ">= 5.6.0"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm-run-all build:default",
    "build:default": "vue-cli-service build --target lib --name coreui-vue ./src/index.js",
    "build:es": "rollup -c --environment TARGET:esm",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit",
    "test:coverage": "vue-cli-service test:unit --coverage",
    "clearCache": "jest --clearCache",
    "release": "npm-run-all clearCache lint build test:coverage generate-hints",
    "generate-hints": "node build/buildVscodeHints.js",
    "auto-changelog": "auto-changelog --template changelog-template.hbs --commit-limit false --package"
  },
  "dependencies": {
    "@coreui/icons": "^1.0.0",
    "@coreui/icons-vue": "^1.1.2",
    "@coreui/utils": "^1.2.1",
    "@popperjs/core": "^2.0.0",
    "lodash.clonedeep": "^4.5.0",
    "perfect-scrollbar": "^1.4.0",
    "tooltip.js": "^1.3.3",
    "vue": "^2.6.11",
    "vue-clickaway2": "^2.3.1",
    "vue-functional-data-merge": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-unit-jest": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/test-utils": "1.0.0-beta.29",
    "auto-changelog": "^1.16.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9",
    "core-js": "^3.6.4",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2",
    "node-sass": "^4.13.0",
    "node-sass-magic-importer": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-license": "^0.13.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-vue": "4.7.2",
    "sass-loader": "^8.0.2",
    "vue-router": "^3.1.4",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "vetur": {
    "tags": "dist/vetur/coreui-tags.json",
    "attributes": "dist/vetur/coreui-attributes.json"
  }
}
